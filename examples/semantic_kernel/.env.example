# Semantic Kernel Standalone Agent Configuration
# Copy this file to .env and fill in your values

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-3.5-turbo

# Agent Configuration
AGENT_NAME=Semantic Kernel Assistant
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=1000

# Plugin Configuration (comma-separated list of plugins to enable)
ENABLED_PLUGINS=math,time,text

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=agent.log